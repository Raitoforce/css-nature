{
  "name": "nature",
  "version": "1.0.0",
  "description": "Landing Page for natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions'npm css/style.concat.css -o css/style.prefix.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raitoforce/css-nature.git"
  },
  "author": "Raitoforce34",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Raitoforce/css-nature/issues"
  },
  "homepage": "https://github.com/Raitoforce/css-nature#readme",
  "devDependencies": {
    "autoprefixer": "^10.3.4",
    "concat": "^1.0.3",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1"
  }
}
